<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>home.home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home.sobre</from-outcome>
            <to-view-id>/sobre.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home.ajuda</from-outcome>
            <to-view-id>/ajuda.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home.adm</from-outcome>
            <to-view-id>/admin/indexAdm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adm.cadastro.funcionario</from-outcome>
            <to-view-id>/admin/cadastros/funcionarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adm.listar.funcionario</from-outcome>
            <to-view-id>/admin/listas/listaFuncionarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adm.cadastro.status</from-outcome>
            <to-view-id>/admin/cadastros/status.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adm.listar.status</from-outcome>
            <to-view-id>/admin/listas/listastatus.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adm.buscar.funcionario</from-outcome>
            <to-view-id>/admin/consultas/consultaFuncionario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adm.buscar.agendamento</from-outcome>
            <to-view-id>/admin/consultas/consultaAgendamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
